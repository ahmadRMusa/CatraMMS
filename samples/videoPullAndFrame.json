{
	"Type": "Process",
	"Label": "1",
	"Variables": {  
		"TitleOfVideo": "My Title of the content",
		"TitleOfFrame": "My Title of the content"
	},
	"Task": {
		"Label": "...",
		"Type": "ContentIngestion",
		"Parameters": {
			"SourceURL": "http://techslides.com/demos/sample-videos/small.mp4",
			"SourceFileName": "123456.mp4",
			"ContentType": "video",
			"userData": { "MyData": "this is my video data" },
			"Title": "${TitleOfVideo}"
		},
		"OnSuccess": {
			"Task": {
				"Type": "Frame",
				"Parameters": {
					"SourceFileName": "123456.jpg",
					"InstantInSeconds": 2.5,
					"userData": { "MyData": "this is my frame data" },
					"Title": "${TitleOfFrame}"
				}
			}
		}
	}
}
